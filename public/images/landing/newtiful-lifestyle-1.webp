	.custom_mkcountdown .timer{
	   background-color: var( --e-global-color-a33205b );border-radius: 2px 2px 2px 2px;padding: 5px 5px 5px 5px;margin-right: calc(10px / 2);font-size: 14px;
	 	font-weight: 600;line-height:12px;color:#fff;text-align: center;
