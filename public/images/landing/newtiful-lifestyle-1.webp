<script>
    document.addEventListener("DOMContentLoaded", function() {
    const countdownElements = document.querySelectorAll('.custom_mkcountdown');

    countdownElements.forEach(countdownElement => {
        let countdownTime = parseInt(countdownElement.getAttribute('data-time')) || 3600;
		let initial_time = parseInt(countdownElement.getAttribute('data-time')) || 3600;
        const savedTime = document.cookie.split('; ').find(row => row.startsWith('countdown_'+initial_time+'='));
        if (savedTime) {
            countdownTime = parseInt(savedTime.split('=')[1]);
        }

        // Update timer
        function updateCountdown() {
            const hoursElement = countdownElement.querySelector('.hours');
            const minutesElement = countdownElement.querySelector('.minutes');
            const secondsElement = countdownElement.querySelector('.seconds');

            const hours = Math.floor(countdownTime / 3600);
            const minutes = Math.floor((countdownTime % 3600) / 60);
            const seconds = countdownTime % 60;

            hoursElement.textContent = String(hours).padStart(2, '0');
            minutesElement.textContent = String(minutes).padStart(2, '0');
            secondsElement.textContent = String(seconds).padStart(2, '0');

            if (countdownTime > 0) {
                countdownTime--;
                document.cookie = `countdown_${initial_time}=${countdownTime}; path=/; max-age=${countdownTime + 1}`;
            } else {
                document.cookie = 'countdown=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;';
            }
        }
        setInterval(updateCountdown, 1000);
    });
});
</script>
	
