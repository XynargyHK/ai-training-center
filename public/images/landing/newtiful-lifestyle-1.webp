				jQuery('.subscription-products').show();

			}    
		});
				

		var delay = 100;
		setTimeout(function() {
			jQuery('.elementor-tab-title').removeClass('elementor-active');
			jQuery('.elementor-tab-content').removeClass('elementor-active');
			jQuery('.elementor-tab-content').css('display', 'none');
		}, delay);
		
		var delay = 0;
		setTimeout(function() {
			jQuery('.elementor-tab-title').removeClass('elementor-active');
			jQuery('.elementor-tab-content').removeClass('elementor-active');
			jQuery('.elementor-tab-content').css('display', 'none');
		}, delay);
		let lastScroll = 0;
		const headerHeight = jQuery('.elementor-location-header').height();
		const whiteHeight = jQuery('#header-part').height();
		
		if(jQuery(window).width() < 1025){
			var topheight = 0;
		} else {
			var topheight = jQuery('#head-top').height();
		}
		var diff = -whiteHeight;
		var diffrev = topheight;
		const handleScroll = () => {
			const currentScroll = window.scrollY;
			
			if (currentScroll > 0) {
				jQuery('.elementor-location-header').addClass('head-sticky');
				if (currentScroll > lastScroll) {
					diffrev = -(whiteHeight);
					if (diff > 1) {
						diff = -(whiteHeight+topheight);
					}
					diff = diff - (currentScroll - lastScroll);
					if (Math.abs(diff) >= (whiteHeight+topheight)) {
						diff = -(whiteHeight+topheight);
					}
					jQuery('.elementor-location-header').css({
						top: diff + 'px'
					});
				} else {
					diff = -topheight;
					let minusval = -(whiteHeight + topheight);
					if (diffrev < minusval) {
						diffrev = -minusval;
					}


					diffrev = diffrev - (currentScroll - lastScroll);
					if (Math.abs(diffrev) < topheight || diffrev > 0) {
						diffrev = -topheight;
					}
					jQuery('.elementor-location-header').css({
						top: diffrev + 'px'
					});
				}
			} else {
				jQuery('.elementor-location-header').removeClass('head-sticky');
			}

			lastScroll = currentScroll;
		};

		window.addEventListener('scroll', handleScroll);
		handleScroll();
		
		
		
		
		try {
			var l = document.querySelectorAll(".sticky_show_after");
			window.addEventListener("scroll", function () {
				l.forEach(function (e) {
					var a = e.dataset && e.dataset.show_after ? e.dataset.show_after : 0; window.scrollY > a ? e.classList.add("visible") : e.classList.remove("visible")
				})
			})
		} catch (e) { console.warn(e) }

	}
	window._AutofillCallbackHandler = window._AutofillCallbackHandler || function(){}
</script>

